{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
      "uni-(.*)": "@/components/uni-$1.vue",
      "mp-html": "@/components/mp-html.vue"
    }
  },
  "pages": [
    {
      "path": "pages/hot/index",
      "style": {
        "navigationBarTitleText": "热门活动"
      }
    },
    {
      "path": "pages/myCreated/index",
      "style": {
        "navigationBarTitleText": "我创建的"
      }
    },
    {
      "path": "pages/myJoin/index",
      "style": {
        "navigationBarTitleText": "我参与的"
      }
    },
    {
      "path": "pages/myFollow/index",
      "style": {
        "navigationBarTitleText": "我关注的"
      }
    },
    {
      "path": "pages/createActivity/index",
      "style": {
        "navigationBarTitleText": "发布活动"
      }
    },
    {
      "path": "pages/activityGroup/index",
      "style": {
        "navigationBarTitleText": "编辑组别"
      }
    },
    {
      "path": "pages/bizHomePage/index",
      "style": {
        "navigationBarTitleText": "Ta的主页"
      }
    },
    {
      "path": "pages/bizAuth/index",
      "style": {
        "navigationBarTitleText": "认证"
      }
    },
    {
      "path": "pages/bizActivityDetail/index",
      "style": {
        "navigationBarTitleText": "活动详情" // 商家活动详情
      },
      "usingComponents": {
        "txv-video": "plugin://tencentvideo/video"
      }
    },
    {
      "path": "pages/visitorActivityDetail/index",
      "style": {
        "navigationBarTitleText": "活动详情" // 游客活动详情
      },
      "usingComponents": {
        "txv-video": "plugin://tencentvideo/video"
      }
    },
    {
      "path": "pages/singupPage/index",
      "style": {
        "navigationBarTitleText": "报名"
      }
    },
    {
      "path": "pages/cashOut/index",
      "style": {
        "navigationBarTitleText": "提现"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8",
    "usingComponents": {
      "txv-video": "plugin://tencentvideo/video"
    }
  },
  "tabBar": {
    "color": "#6a6a6a",
    "selectedColor": "#0066FF",
    "backgroundColor": "#ffffff",
    "fontSize": "16px",
    "list": [
      {
        "pagePath": "pages/hot/index",
        "selectedIconPath": "static/hot_on.png",
        "iconPath": "static/hot.png",
        "text": "热门活动"
      },
      {
        "pagePath": "pages/myCreated/index",
        "selectedIconPath": "static/myCreated_on.png",
        "iconPath": "static/myCreated.png",
        "text": "我创建的"
      }
